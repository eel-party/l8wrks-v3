---
layout: layout
---

<p><small style="color: red;">this is using the sub-layout template</small></p>

{{ content | safe }}

<h3>Tagged:</h3>

{% for tag in collections.tagList %}
  {% set tagUrl %}/tags/{{ tag }}/{% endset %}
  <a href="{{ tagUrl | url }}" class="post-tag">{{ tag }}</a>
{% endfor %}